{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "S3Bucket": {
            "Type": "AWS::S3::Bucket"
        }
    },
    "Outputs": {
        "BucketName": {
            "Value": {
                "Ref": "S3Bucket"
            },
            "Description": "Name of the sample Amazon S3 bucket."
        }
    }
    "myvpc" :{
  "Type" : "AWS::EC2::VPC",
  "Properties" : {
      "CidrBlock" : "11.0.0.0/16",
      "EnableDnsHostnames" : "true",
      "EnableDnsSupport" : "true",
         "Tags" : [ 
         {"Key" : "stack", "Value" : "production"} 
      ]
}
      
    
}
}
